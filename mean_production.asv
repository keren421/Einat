 h = gcf;
axesObjs = get(h, 'Children');
dataObjs = get(axesObjs, 'Children');
lines = dataObjs{2};

t = cell(length(lines),1);
production = cell(length(lines),1);
run_type = cell(length(lines),1);
average_production = nan(length(lines),1);

for i = 1:length(lines)
    t{i} = line(i).XData;
    production{i} = line(i).YData;
    average_production(i) = trapz(t,production)?
    run_type{i} = line(i).DiaplayName;
end